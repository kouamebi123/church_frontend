{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://church-production-9a34.up.railway.app",
    "REACT_APP_SOCKET_URL": "https://church-production-9a34.up.railway.app/socket.io"
  }
}