{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "@vite_api_url",
    "VITE_BACKEND_URL": "@vite_backend_url",
    "VITE_HIDE_CONSOLE": "true",
    "VITE_PERFORMANCE_TRACKING": "false"
  },
  "buildCommand": "npm run build",
  "outputDirectory": "build"
}