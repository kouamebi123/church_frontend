{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@mui/x-date-pickers": "^7.28.3",
    "@reduxjs/toolkit": "^2.6.1",
    "@svgr/plugin-svgo": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@tsparticles/react": "^3.0.0",
    "@vercel/speed-insights": "^1.2.0",
    "aos": "^2.3.4",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "formik": "^2.4.6",
    "framer-motion": "^12.7.5",
    "leaflet": "^1.9.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-organizational-chart": "^2.2.1",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.9.3",
    "react-scripts": "^0.0.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "resolve-url-loader": "^5.0.0",
    "socket.io-client": "^4.8.1",
    "swiper": "^11.2.6",
    "tsparticles": "^3.8.1",
    "web-vitals": "^2.1.4",
    "yarn": "^1.22.22",
    "yup": "^1.6.1"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build && node scripts/generate-sw.js",
    "build:sw": "node scripts/generate-sw.js",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest --coverage --run",
    "test:ci": "vitest --coverage --run --reporter=verbose",
    "test:debug": "vitest --debug",
    "test:watch": "vitest --watch",
    "test:components": "vitest --run src/__tests__/components",
    "test:hooks": "vitest --run src/__tests__/hooks",
    "test:services": "vitest --run src/__tests__/services",
    "test:utils": "vitest --run src/__tests__/utils",
    "test:e2e": "vitest --run src/__tests__/e2e",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx vite-bundle-analyzer build/assets/*.js",
    "precommit": "npm run lint:check && npm run format:check && npm run test:ci"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.19",
    "@types/node": "^24.7.0",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^8.57.1",
    "prettier": "^3.3.3",
    "vite": "^7.1.9"
  }
}
